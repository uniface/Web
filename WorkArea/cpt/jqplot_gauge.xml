<DAT name="USCRIPT" xml:space='preserve'>;--------------------------------------------------------
;
;--------------------------------------------------------
operation render
;--------------------------------------------------------
public web

params
	Struct pOptions : IN
endparams

variables
	String vOptionsJson
endvariables

	;Convert pOptions to a JSON string ready to go to
	;the browser.
	structToJson vOptionsJson, pOptions
	;TODO: Catch error and raise exception

	webactivate $instancename.renderJs(vOptionsJson)

	return 0

end ;render


;--------------------------------------------------------
;
;--------------------------------------------------------
weboperation renderJs
;--------------------------------------------------------
params
	String pOptionString : IN
endparams

javascript

var thisInstance = this;

setTimeout(function(){

  //Parse the options string into JSON
  var options = JSON.parse(pOptionString);

  //Get the ID we're going to use to attach the chart to
  var attachField = thisInstance.getEntity("TECHNICAL.DUMMY").getOccurrence(0).getField("ATTACH_DIV");

  //Set a class against the field so we can search for it
  var className = thisInstance.getName() + '_attachDiv';
  attachField.setProperty("class:" + className, true);
  attachField.setProperty("class:jqplot-target", true);

  //Find the dom node with the class above store it's ID
  var id = $('.' + className).attr('id');

  //Escape special characters in the id so that JQuery reads it as an id
  id = id.replace(/(:|\.|\[|\]|,)/g, "\\$1");

  s1 = [options.value];

  var gauge = $.jqplot(id,[s1],{
    seriesDefaults: {
      renderer: $.jqplot.MeterGaugeRenderer,
      rendererOptions: {
        min: 0,
        max: 50,
        intervals:[16.3, 32.6, 50],
        intervalColors:['#93b75f', '#E7E658', '#cc6666'],
        label: options.label,
        labelPosition: 'bottom',
        labelHeightAdjust: -5,
      }
    }
  });
},1000);
endjavascript

end ;renderJs



trigger preActivate
