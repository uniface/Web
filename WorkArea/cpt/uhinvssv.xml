endvariables
#define  OuterEntity = INVHEADER
#define  PagePrimaryKey = ID
#define  dq = %%"

#define XMLresponseheader = &lt;?xml version='1.0' encoding='utf-8'?&gt;&lt;rows&gt;&lt;page&gt;%%$pageno$%%%&lt;/page&gt;&lt;total&gt;%%$lastpage$%%%&lt;/total&gt;&lt;records&gt;%%$pagelimit$%%%&lt;/records&gt;

#define  jsonresponseheader = 	{&lt;dq&gt;page&lt;dq&gt;:&lt;dq&gt;%%$pageno$%%%&lt;dq&gt;,&lt;dq&gt;total&lt;dq&gt;:%%$lastpage$%%%,&lt;dq&gt;records&lt;dq&gt;:&lt;dq&gt;%%$pagelimit$%%%&lt;dq&gt;,&lt;dq&gt;rows&lt;dq&gt;:
</DAT>
<DAT name="USCRIPT" xml:space='preserve'>entry loadpage
	returns numeric
	clear/e "&lt;OuterEntity&gt;"

	if(($Pageno$="")| ($Pageno$ ="1"))
		$pageno$ = 1
		$PageLimit$ = 0   
		$lastpage$=0 
		selectdb count(&lt;PagePrimaryKey&gt;) from "&lt;OuterEntity&gt;" u_condition $condition$ to $PageLimit$ 
	endif
	$LastPage$ = $PageLimit$ / $PageSIze$
	if($lastpage$[fraction]) 
		$LastPage$ = $LastPage$[trunc]
		$lastpage$+=1
	else
		$LastPage$= $LastPage$[trunc]
	endif
	
	if(($Pageno$ &lt; 0)|($Pageno$ &gt; $LastPage$)) $Pageno$=$LastPage$
	$xoffset$ = ($Pageno$ - 1) * $PageSIze$
	retrieve/e "&lt;OuterEntity&gt;"
	return($status)
end

entry setparms
	returns boolean
	params
		string pParms: IN
	endparams
	variables
		string vParms, vnd, searchField, searchString, SearchOper
		numeric vid
	endvariables
	
	if(pParms = "") return(1)
	
	searchfield = ""
	searchstring = ""
	searchOper = ""
	$condition$ = ""
	$search$=0
	$sortorder$="desc"
  	$sortfield$ = "ID"
	$pageno$ = 1
	$pagesize$ = 10
	$callback$=""
   	getitem/id $callback$, 			pparms, "callback"

	vparms=$replace(pParms,1,"{","", -1)
	vparms=$replace(vparms,1,"}","", -1)
	vparms=$replace(vparms,1,"%%"","", -1)
	vparms=$replace(vparms,1,":","=", -1)
	vparms=$replace(vparms,1,",","&uSEP;", -1)
	getitem/id $pageno$,     			vparms, "page"
	getitem/id $pagesize$,     		vparms, "rows"
	getitem/id $sortorder$,         	vparms, "sord"    ;Sort order asc or desc
	getitem/id $sortfield$,         	vparms, "sidx"    ;Order by field
	getitem/id $search$,     			vparms, "_search"
	getitem/id vnd,         			vparms, "nd"
	getitem/id vid,     				vparms, "id"
	getitem/id $pagelimit$           vparms, "totalRecords"

	getitem/id searchfield,       	vparms, "searchField" 
	getitem/id searchstring,     	vparms, "searchString"
	getitem/id searchOper,         vparms, "searchOper"
;	getitem/id $filters$,         	vparms, "filters"
	
	if($search$)
       selectcase searchoper
		case "eq"
			$condition$ = "%%searchfield%%% = '%%searchstring%%%'"
		case "ne"
			$condition$ = "%%searchfield%%% != '%%searchstring%%%'"
		case "lt"
			$condition$ = "%%searchfield%%% &lt; '%%searchstring%%%'"
		case "le"
			$condition$ = "%%searchfield%%% &lt;= '%%searchstring%%%'"
		case "gt"
			$condition$ = "%%searchfield%%% &gt; '%%searchstring%%%'"
		case "ge"
			$condition$ = "%%searchfield%%% &gt;= '%%searchstring%%%'"
		case "bw"
			$condition$ = "%%searchfield%%% = '%%searchstring%%%&uALL;'"
		case "ew"
			$condition$ = "%%searchfield%%% = '&uALL;%%searchstring%%%'"
		case "cn"
			$condition$ = "%%searchfield%%% = '&uALL;%%searchstring%%%&uALL;'"
		endselectcase
	endif
	if(vid != "" )
       $search$=1
		$condition$ = "id = %%vid%%%"
	endif
	
	return(0)
end

entry localsave
	returns numeric
	params
		numeric pstatus : IN
		numeric pstat : OUT
		string  pMessage : OUT
	endparams
	
	if(pstatus != 0)
		pstat = $idpart($update_error$)
		pmessage = $valuepart($update_error$)
		return(0)
	else
		store
		if ($status &lt; 0)
			pstat = $status
			pmessage = $text(1500)
			rollback
			return(0)
		else
			if ($status = 1)
				pstat = $status
				pmessage = $text(1723) ;no change
			else
				commit
				if ($status &lt; 0)
					rollback
					return(0)
				else
					pstat = $status
					pmessage = $text(1805) ; OK
					return(0)
				endif
			endif
		endif
	endif
end



operation get
	params
		string pParms:     IN
		string pFmt:        IN
		string pData:     OUT
	endparams
	variables
		string vData, vstring
		struct sinvheader
	endvariables
	
	if(setparms(pParms))
		pData = "No parameters"
		return(0)
	endif
	
	if(loadpage()) 
		pData = "Retrieve failed    maxhits=%%$pagesize$%%% offset=%%$xoffset$%%%  "
		return(0)
	endif
	
	componentToStruct sinvheader, "&lt;OuterEntity&gt;"
	if($uppercase(pFmt)="XML")
		sinvheader-&gt;$name = "&lt;OuterEntity&gt;"
		structtoxml  vData, sinvheader
		vstring="&lt;xmlresponseheader&gt;"
		pData=$concat(vstring, vData, "&lt;/rows&gt;")
	else   ;Assume JSON ------------
		sinvheader-&gt;OCC-&gt;$name = ""
		structtojson  vData, sinvheader
		vstring=$concat("&lt;jsonresponseheader&gt;",vData, "}")
      	if($callback$ !="")  ;it's jsonp
			pData=$concat($callback$,"(",vString,");")
		else
			pData=vstring
		endif

	endif
	
	return(0)
end


operation set
	params
		string pdata : IN
		numeric pstat :OUT
		string pMessage :OUT
	endparams
	variables
		struct vstruct, vdata
		numeric a
	endvariables
	clear/e "&lt;OuterEntity&gt;"
	jsontostruct vstruct, pdata
	
	if(vstruct-&gt;*{1}-&gt;$isleaf)    ;single row
		vstruct-&gt;$name="OCC"
		vdata-&gt;$name="&lt;OuterEntity&gt;"
		vstruct-&gt;$parent = vdata
		structtocomponent vdata
		$occcrc("&lt;OuterEntity&gt;") = UCRC
		$occstatus("&lt;OuterEntity&gt;") = USTAT
		
	else     ;multiple rows
		
		vstruct-&gt;$name="&lt;OuterEntity&gt;"
		vstruct-&gt;*-&gt;$name="OCC"
		
		structtocomponent vstruct
		a=$totocc("&lt;OuterEntity&gt;")
		setocc "&lt;OuterEntity&gt;",a
		repeat
			if(USTAT = "est")
				remocc "&lt;OuterEntity&gt;",a
			else
				$occcrc("&lt;OuterEntity&gt;") = UCRC
				$occstatus("&lt;OuterEntity&gt;") = USTAT
			endif
			a-=1
			setocc "&lt;OuterEntity&gt;",a
		until(($status &lt;1)|(a &lt;1))
		
	endif
	
	retrieve/reconnect "&lt;OuterEntity&gt;"
	return(localsave($status, pstat,pmessage))

end

operation del
	params
		string pdata : IN
		numeric pstat :OUT
		string pMessage :OUT
	endparams
	variables
		struct vstruct, vdata
	endvariables
	clear/e "&lt;OuterEntity&gt;"
	jsontostruct vstruct, pdata
	vstruct-&gt;$name="OCC"    
	
	vdata-&gt;$name="&lt;OuterEntity&gt;"
	vstruct-&gt;$parent = vdata
	structtocomponent vdata
	$occcrc("&lt;OuterEntity&gt;") = UCRC
	$occstatus("&lt;OuterEntity&gt;") = USTAT
	retrieve/reconnect "&lt;OuterEntity&gt;"
	return(localsave($status, pstat,pmessage)) 
end

operation add
	params
		string pdata : IN
		numeric pstat :OUT
		string pMessage :OUT
	endparams
	variables
		struct vstruct, vdata
		numeric vnewid
	endvariables
	clear/e "&lt;OuterEntity&gt;"
	jsontostruct vstruct, pdata
	vstruct-&gt;$name="OCC"
	selectdb max(&lt;PagePrimaryKey&gt;) from "&lt;OuterEntity&gt;"  to vnewid
	; simple mechanism to get the next ID by finding the maximum value of field
	; of existing occurencies.
	vnewid+=1 
	
