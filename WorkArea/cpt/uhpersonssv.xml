<DAT name="UDECLARATIONS" xml:space='preserve'>variables
  string          CONDITION                
  string          OPTIONS                  
  string          SORT                     
  string          UPDATE_ERROR             
endvariables
#define  OuterEntity = PERSON
#define  PagePrimaryKey = ID
#define  dq = %%"

#define XMLresponseheader = &lt;?xml version='1.0' encoding='utf-8'?&gt;&lt;rows&gt;&lt;page&gt;%%vpageno%%%&lt;/page&gt;&lt;total&gt;%%vlastpage%%%&lt;/total&gt;&lt;records&gt;%%vpagelimit%%%&lt;/records&gt;

#define  jsonresponseheader = {&lt;dq&gt;page&lt;dq&gt;:&lt;dq&gt;%%vpageno%%%&lt;dq&gt;,&lt;dq&gt;total&lt;dq&gt;:%%vlastpage%%%,&lt;dq&gt;records&lt;dq&gt;:&lt;dq&gt;%%vpagelimit%%%&lt;dq&gt;,&lt;dq&gt;rows&lt;dq&gt;:
</DAT>
<DAT name="USCRIPT" xml:space='preserve'>entry loadpage
	returns numeric
	params
		string pFmt  : IN
		string pData : INOUT
		struct pstructparams : INOUT
	endparams
	variables
		numeric vPageno, vPageLimit, vlastpage,  vpagesize, voffset
		string vsortfield, vsortorder, vstring, vcondition,vData
		struct hservice
	endvariables
	vPageno         = pstructparams-&gt;page
	vPageLimit     = pstructparams-&gt;totalRecords
	vcondition    = pstructparams-&gt;condition
	vPagesize     = pstructparams-&gt;rows
	vPageno         = pstructparams-&gt;page
	vsortfield     = pstructparams-&gt;sidx
	vsortorder    = pstructparams-&gt;sord
	
	clear/e "&lt;OuterEntity&gt;"
	
	if((vPageno="")| (vPageno ="1"))
		vPageno = 1
		vPageLimit = 0   
		vlastpage=0 
		selectdb count(&lt;PagePrimaryKey&gt;) from "&lt;OuterEntity&gt;" u_condition vcondition to vPageLimit 
	endif
	vLastPage = vPageLimit / vPageSize
	if(vlastpage[fraction]) 
		vLastPage = vLastPage[trunc]
		vlastpage+=1
	else
		vLastPage= vLastPage[trunc]
	endif
	
	if((vPageno &lt; 0)|(vPageno &gt; vLastPage)) vPageno=vLastPage
	voffset = (vPageno - 1) * vPageSize
	pstructparams-&gt;totalRecords = vPageLimit
	$options$ = "maxhits=%%vpagesize%%%&uSEP;offset=%%voffset%%%"
	$sort$ = "%%vsortfield%%% %%vsortorder%%%"
	$condition$ = vcondition
	retrieve/e "&lt;OuterEntity&gt;"
	if(!$status)
		componentToStruct hservice, "&lt;OuterEntity&gt;"
		if($uppercase(pFmt)="XML")
			hservice-&gt;$name = "&lt;OuterEntity&gt;"
			structtoxml  vData, hservice
			vstring="&lt;xmlresponseheader&gt;"
			pData=$concat(vstring, vData, "&lt;/rows&gt;")
		else
			hservice-&gt;OCC-&gt;$name = ""
			structtojson  vData, hservice
			vstring="&lt;jsonresponseheader&gt;"
			pData=$concat(vstring, vData, "}")
		endif
	else
		pstructparams-&gt;status = $status
		return($status)
	endif
end

entry setparms
	returns boolean
	params
		string pParms: IN
		struct pstructparams: OUT
	endparams
	variables
		string vsearchField, vsearchString, vSearchOper, vCondition
	endvariables
	
	if(pParms = "") return(1)
	
	jsontostruct pstructparams, pParms
	if(pstructparams-&gt;sidx = "")
		pstructparams-&gt;sidx = "id"
		pstructparams-&gt;sord = "asc"
	endif
	
	if(pstructparams-&gt;_search)
		vsearchoper = pstructparams-&gt;searchOper
		vsearchfield = pstructparams-&gt;searchField
		vsearchString = pstructparams-&gt;searchString
		selectcase vsearchoper
			case "eq"
				vcondition = "%%vsearchfield%%% = '%%vsearchstring%%%'"
			case "ne"
				vcondition = "%%vsearchfield%%% != '%%vsearchstring%%%'"
			case "lt"
				vcondition = "%%vsearchfield%%% &lt; '%%vsearchstring%%%'"
			case "le"
				vcondition = "%%vsearchfield%%% &lt;= '%%vsearchstring%%%'"
			case "gt"
				vcondition = "%%vsearchfield%%% &gt; '%%vsearchstring%%%'"
			case "ge"
				vcondition = "%%vsearchfield%%% &gt;= '%%vsearchstring%%%'"
			case "bw"
				vcondition = "%%vsearchfield%%% = '%%vsearchstring%%%&uALL;'"
			case "ew"
				vcondition = "%%vsearchfield%%% = '&uALL;%%vsearchstring%%%'"
			case "cn"
				vcondition = "%%vsearchfield%%% = '&uALL;%%vsearchstring%%%&uALL;'"
		endselectcase
		pstructparams-&gt;condition = vcondition
	endif
		
	return(0)
end

entry localsave
	returns numeric
	params
		numeric pstatus : IN
		numeric pstat : OUT
		string  pMessage : OUT
	endparams
	
	if(pstatus != 0)
		pstat = $idpart($update_error$)
		pmessage = $valuepart($update_error$)
		return(0)
	else
		store
		if ($status &lt; 0)
			pstat = $status
			pmessage = $text(1500)
			rollback
			return(0)
		else
			if ($status = 1)
				pstat = $status
				pmessage = $text(1723) ;no change
			else
				commit
				if ($status &lt; 0)
					rollback
					return(0)
				else
					pstat = $status
					pmessage = $text(1805) ; OK
					return(0)
				endif
			endif
		endif
	endif
end



operation get
	params
		string pParms:    IN
		string pFmt:      IN
		string pData:     OUT
	endparams
	variables
		struct vstructparams
	endvariables
	
	if(setparms(pParms, vstructparams))
		pData = "No parameters"
		return(0)
	endif
	
	if(loadpage(pFmt, pData, vstructparams)) 
		pData = "Retrieve failed    status=%%vstructparams-&gt;status%%% "
		return(0)
	endif
	
	return(0)
end
